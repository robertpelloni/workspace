{
	"mcpServers": {
		"serena": {
			"type": "local",
			"command": "uv",
			"tools": [
				"*"
			],
			"args": [
				"run",
				"--directory",
				"C:\\Users\\hyper\\serena\\",
				"serena",
				"start-mcp-server",
				"--context",
				"agent",
				"--project",
				"C:\\Users\\hyper\\fwber\\"
			]
		},
		"sequential-thinking": {
			"type": "local",
			"command": "npx",
			"tools": [
				"*"
			],
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"codex-mcp-server": {
			"type": "local",
			"command": "npx",
			"tools": [
				"*"
			],
			"args": [
				"-y",
				"codex-mcp-server"
			]
		},
		"gemini-mcp-tool": {
			"type": "local",
			"command": "npx",
			"tools": [
				"*"
			],
			"args": [
				"-y",
				"gemini-mcp-tool"
			]
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"C:\\Users\\hyper\\fwber\\"
			],
			"tools": [
				"*"
			],
			"timeout": 30000,
			"startupTimeout": 60000
		},
		"memory": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			],
			"tools": [
				"*"
			],
			"timeout": 30000,
			"startupTimeout": 60000
		},
		"everything": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-everything"
			],
			"tools": [
				"*"
			],
			"timeout": 30000,
			"startupTimeout": 60000
		},
		"zen-mcp-server": {
			"command": "uv",
			"args": [
				"run",
				"--directory",
				"C:\\Users\\hyper\\zen-mcp-server\\",
				"zen-mcp-server"
			],
			"tools": [
				"*"
			],
			"env": {
				"OPENAI_API_KEY": "${OPENAI_API_KEY}",
				"OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
				"GEMINI_API_KEY": "${GEMINI_API_KEY}"
			},
			"timeout": 30000,
			"startupTimeout": 60000
		},
		"chroma-knowledge": {
			"command": "python",
			"args": [
				"-m",
				"chroma_mcp_server"
			],
			"tools": [
				"*"
			],
			"env": {
				"CHROMA_CLIENT_TYPE": "http",
				"CHROMA_HOST": "localhost",
				"CHROMA_PORT": "8000"
			},
			"timeout": 30000,
			"startupTimeout": 60000
		}
	}
}